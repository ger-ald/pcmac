OBJ = masm.o stable.o utilits.o error.o init.o file.o pass.o getline.o libmak.o libget.o vars.o express.o generate.o primitiv.o tfunc.o objmad.o
INC = include.h defines.h functype.h types.h vars.h tfunc.h
MODEL=l
INCLUDE=include
LIB=lib
OPTIONS=-c -N -I$(INCLUDE) -K  -f-  -G -m$(MODEL) -Z
masm.exe		: $(OBJ)
	tcc -L$(LIB) -m$(MODEL) -f- $(OBJ)
masm.o 			: masm.c 	$(INC)
	tcc $(OPTIONS)  -c masm.c
stable.o 		: stable.c 	$(INC)
	tcc $(OPTIONS)  -c stable.c
utilits.o 		: utilits.c  	$(INC)
	tcc $(OPTIONS)  -c utilits.c
error.o 		: error.c 	$(INC)
	tcc $(OPTIONS)  -c error.c
init.o 			: init.c 	$(INC)
	tcc $(OPTIONS)  -c init.c
file.o 			: file.c 	$(INC)
	tcc $(OPTIONS)  -c file.c
pass.o 			: pass.c 	$(INC)
	tcc $(OPTIONS)  -c pass.c
getline.o 		: getline.c  	$(INC)
	tcc $(OPTIONS)  -c getline.c
libmak.o 		: libmak.c 	$(INC)
	tcc $(OPTIONS)  -c libmak.c
libget.o 		: libget.c 	$(INC)
	tcc $(OPTIONS)  -c libget.c
vars.o 			: vars.c 	$(INC)
	tcc $(OPTIONS)  -c vars.c
express.o 		: express.c  	$(INC)
	tcc $(OPTIONS)  -c express.c
generate.o 		: generate.c 	$(INC)
	tcc $(OPTIONS)  -c generate.c
primitiv.o 		: primitiv.c 	$(INC)
	tcc $(OPTIONS)  -c primitiv.c
tfunc.o 		: tfunc.c 	 $(INC)
	tcc $(OPTIONS)  -c tfunc.c
objmad.o		: objmad.c	$(INC)
	tcc $(OPTIONS)  -c objmad.c
vars.h 			: vars.c
	extmaker vars.c vars.h
